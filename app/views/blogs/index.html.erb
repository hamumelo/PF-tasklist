<div class="toppage">
<h1 class="top">カレンダー</h1>



<div class="col-md-8">
<%= month_calendar events: @blogs do |date, blogs| %>
  <%= date.day %>

    <div>
      <%= link_to '＋', new_blog_path(date) %>
    </div>
  <% blogs.each do |blog| %>
    <div>
      <%= link_to blog.title, blog %>
    </div>
  <% end %>
<% end %>
</div>
</div>
</div>

