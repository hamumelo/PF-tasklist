<h2>Log in</h2>

<%= form_with model: @user, url: user_session_path, local: true do |f| %>

  <div class="name_pass">
    <%= f.label :氏名 %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :氏名パス %>
    <%= f.text_field :name_pass %>
  </div>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>